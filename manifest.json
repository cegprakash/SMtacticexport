{
  "name": "Striker Manager Formation Swap and Tactics Export",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Striker Manager Tactics and Formation enhancement",
  "content_scripts": [
    {
      "matches" : ["http://*.strikermanager.com/tactica2.php?id_tactica=*"],
      "all_frames" : true,
      "js":["tacticexport.js"]
    },
    {
      "matches" : ["http://*.strikermanager.com/formacion2.php?*"],
      "all_frames" : true,
      "js":["formationswap.js"]
    }
  ],
  "permissions": [
    "http://*.strikermanager.com/",
    "storage",
    "tabs"
  ]
}